@model  IEnumerable<Category>
@{
    ViewData["Title"] = "Category Index";
}

<h>Hi I am forn category </h>

<table class="table table-bordered table-striped" style="width: 100%">
    <thead>
        <tr>
            <th>Category Name</th>
            <th>Display Order</th>
        </tr>    
    </thead>
    <tbody>
        @foreach(var obj in Model)
        {
            <tr>
                <td width="50%">
                    @obj.Name
                </td>
                <td width="50%">
                    @obj.DispalyOrder
                </td>
            </tr>
        }
    </tbody>
</table>